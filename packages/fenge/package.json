{
  "name": "fenge",
  "version": "0.1.1",
  "description": "A CLI tool for code quality",
  "keywords": [
    "cli",
    "quality",
    "typescript",
    "format",
    "lint",
    "eslint",
    "prettier",
    "biome",
    "style",
    "code-style",
    "validator",
    "pre-commit",
    "husky",
    "lint-staged",
    "xo",
    "standard",
    "modern"
  ],
  "homepage": "https://github.com/zanminkian/fenge",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zanminkian/fenge.git"
  },
  "license": "MIT",
  "author": "hellozmj@qq.com",
  "type": "module",
  "exports": {
    "./eslint-config": "./src/re-export/eslint.config.js",
    "./prettier-config": "./src/re-export/prettier.config.js",
    "./types": {
      "types": "./types/index.d.ts"
    },
    "./tsconfig": "./tsconfig/tsconfig.json",
    "./tsconfig/*": "./tsconfig/*.json",
    "./tsconfig/*.json": "./tsconfig/*.json"
  },
  "bin": "./src/bin/cli.js",
  "scripts": {
    "build": "tsc --noEmit -p tsconfig.build.json"
  },
  "dependencies": {
    "@fenge/eslint-config": "workspace:*",
    "@fenge/prettier-config": "workspace:*",
    "@fenge/tsconfig": "workspace:*",
    "@fenge/types": "workspace:*",
    "chalk": "5.3.0",
    "commander": "12.1.0",
    "eslint": "8.57.0",
    "lilconfig": "3.1.2",
    "lint-staged": "15.2.10",
    "ora": "8.1.0",
    "prettier": "3.3.3",
    "prettier-ignore": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "22.7.5"
  },
  "peerDependencies": {
    "typescript": "^5.6.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  }
}
